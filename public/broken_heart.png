<span class="hljs-comment"># ASCII Heart Animation Function</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">heart_animation</span>():
    heart = [
        <span class="hljs-string">"  *****     *****  "</span>,
        <span class="hljs-string">"*******   ******* "</span>,
        <span class="hljs-string">"*******   ******* "</span>,
        <span class="hljs-string">" **************** "</span>,
        <span class="hljs-string">"  **************  "</span>,
        <span class="hljs-string">"   ************   "</span>,
        <span class="hljs-string">"    **********    "</span>,
        <span class="hljs-string">"      ******      "</span>,
        <span class="hljs-string">"       ****       "</span>,
        <span class="hljs-string">"        **        "</span>
    ]
    <span class="hljs-keyword">for</span> line <span class="hljs-keyword">in</span> heart:
        <span class="hljs-built_in">print</span>(<span class="hljs-string">"\033[91m"</span> + line + <span class="hljs-string">"\033[0m"</span>)  <span class="hljs-comment"># Print in red color</span>
        time.sleep(<span class="hljs-number">0.3</span>)

<span class="hljs-comment"># Love Compatibility Calculator</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">love_calculator</span>(<span class="hljs-params">name1, name2</span>):
    love_score = random.randint(<span class="hljs-number">50</span>, <span class="hljs-number">100</span>)  <span class="hljs-comment"># Generate love percentage between 50-100</span>
    <span class="hljs-built_in">print</span>(<span class="hljs-string">f"\n💞 Love Compatibility between <span class="hljs-subst">{name1}</span> &amp; <span class="hljs-subst">{name2}</span>: <span class="hljs-subst">{love_score}</span>% 💞"</span>)
    <span class="hljs-keyword">if</span> love_score &gt; <span class="hljs-number">85</span>:
        <span class="hljs-built_in">print</span>(<span class="hljs-string">"🔥 You two are a perfect match! 🔥"</span>)
    <span class="hljs-keyword">elif</span> love_score &gt; <span class="hljs-number">70</span>:
        <span class="hljs-built_in">print</span>(<span class="hljs-string">"😍 A lovely couple with strong chemistry! 😍"</span>)
    <span class="hljs-keyword">else</span>:
        <span class="hljs-built_in">print</span>(<span class="hljs-string">"💖 Love takes effort, but it's worth it! 💖"</span>)

<span class="hljs-comment"># AI-generated Love Poem</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">love_poem</span>(<span class="hljs-params">name1, name2</span>):
    poems = [
        <span class="hljs-string">f"💌 My dearest <span class="hljs-subst">{name2}</span>,\nWhen I see you, my heart takes flight,\nLike stars that dance in endless night.\n<span class="hljs-subst">{name1}</span> &amp; <span class="hljs-subst">{name2}</span>, a story so true,\nForever together, just me and you. 💕"</span>,
        
        <span class="hljs-string">f"💘 <span class="hljs-subst">{name1}</span> and <span class="hljs-subst">{name2}</span>, a tale so sweet,\nFate has made our hearts complete.\nThrough every smile and every glance,\nTogether, we share a lovely dance. 💖"</span>,

        <span class="hljs-string">f"💓 <span class="hljs-subst">{name2}</span>, your love shines so bright,\nLike a moonbeam in the darkest night.\n<span class="hljs-subst">{name1}</span>'s heart beats just for you,\nA love so deep, so pure, so true. 💕"</span>
    ]
    <span class="hljs-built_in">print</span>(<span class="hljs-string">"\n"</span> + random.choice(poems))  <span class="hljs-comment"># Pick a random poem</span>

<span class="hljs-comment"># Main Function</span>
